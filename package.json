{
  "name": "ooxast-edge",
  "version": "0.0.0",
  "description": "OOXML AST type definitions, powered by Demo Macro",
  "private": true,
  "scripts": {
    "build": "pnpm -r prepack",
    "dev": "pnpm -r dev",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "example": "vite-node playground/src/index.ts",
    "lint": "basis lint --all",
    "edge": "pnpm -r exec bump version",
    "release": "pnpm -r exec bump version -r patch",
    "public": "pnpm -r --no-bail exec bump publish",
    "postinstall": "basis git setup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/docenjs/ooxast.git"
  },
  "keywords": [],
  "author": {
    "name": "Demo Macro",
    "email": "abc@imst.xyz",
    "url": "https://imst.xyz/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/docenjs/ooxast/issues"
  },
  "homepage": "https://github.com/docenjs/ooxast#readme",
  "devDependencies": {
    "@funish/basis": "0.1.1",
    "@prettier/plugin-oxc": "0.0.4",
    "@prettier/plugin-xml": "3.4.2",
    "@types/node": "24.3.3",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "oxlint": "1.15.0",
    "oxlint-tsgolint": "0.2.0",
    "prettier": "3.6.2",
    "unbuild": "3.6.1",
    "vite-node": "3.2.4",
    "vitest": "3.2.4"
  }
}
